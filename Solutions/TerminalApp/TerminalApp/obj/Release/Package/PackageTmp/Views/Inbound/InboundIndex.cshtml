@model TerminalApp.Models.Entities.Inbound
@{
    ViewBag.Title = "InboundIndex";
}

<br />
<br />
<div>
    @using (Html.BeginForm("ConteynerIdCheck", "Inbound", FormMethod.Post))
    {

        <table>
            <tr>
                <td><p>Conteyner Code:</p></td>
                <td>@Html.TextBoxFor(x => x.InboundConteynerId)</td>
                <td><p class="text-danger">@ViewBag.Error</p></td>
            </tr>
            <tr>
                <td>
                    <input type="submit" value="Giriş" class="btn btn-primary" id="btnSubmit" />

                </td>
            </tr>

        </table>
    }
</div>