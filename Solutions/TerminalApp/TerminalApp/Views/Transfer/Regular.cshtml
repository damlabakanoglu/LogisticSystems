@model TerminalApp.Models.Entities.Order
@{
    ViewBag.Title = "Regular";
}

<br />
<br />
<div>
    @using (Html.BeginForm("OrderIdCheck", "Transfer", FormMethod.Post))
    {

        <table>
            <tr>
                <td><p>Order Id :</p></td>
                <td>@Html.TextBoxFor(x => x.Id)</td>
                <td><p class="text-danger">@ViewBag.Error</p></td>
            </tr>
            <tr>
                <td>
                    <input type="submit" value="Giriş" class="btn btn-primary" id="btnSubmit" />

                </td>
            </tr>

        </table>
    }
</div>