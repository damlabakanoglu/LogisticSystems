@model TerminalApp.Models.Entities.Sku
@{
    ViewBag.Title = "PozitionAuditPage";
}


<br />
<br />
<div>
    @using (Html.BeginForm("PozitionAudit", "Audit", FormMethod.Post))
    {

        <table>
            <tr>
                <td><p>Sku :</p></td>
                <td>@Html.TextBoxFor(x => x.Sku1)</td>


            </tr>
            <tr>
                <td><p>Sku Piece :</p></td>
                <td>@Html.TextBoxFor(x => x.SkuPiece)</td>
                <td><p class="text-danger">@ViewBag.Error</p></td>
            </tr>
            <tr>

                <td>
                    <input type="submit" value="Giriş" class="btn btn-primary" id="btnSubmit" />
                </td>
            </tr>

        </table>


    }
</div>